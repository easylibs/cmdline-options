

group "OPTIONS" {

	compound "-p"
	compound "-m"
	compound "-foption..."
	compound "-Wwarn..."
	compound "-Dmacro[=defn]..."
	compound "-mmachine-option..."
	compound "-Umacro"
	compound "-Olevel"
	
	group "main" {
		group "Overall Options" {
			"-c"
			"-S"
			"-E"
			"-o file"
			"-x language:language"
			"-v"
			"--target-help"
			"--version"
			"-pass-exit-codes"
			"-spec=file"
			"-wrapper @file"
			"-fplugin=file"
			"-fada-spec-parent=unit"
			"-fdump-go-spec=file"
		}
		
		group "C++ Language Options" {
			"-fabi-version=n:int"
			"-fno-access-control"
			"-faligned-new=n:int"
			"-fargs-in-order=n:int"
			"-fcheck-new"
			"-fconstexpr-depth=n:int"
			"-fconstexpr-loop-limit=n:int"
			"-ffriend-injection"
			"-fno-elide-constructors"
			"-fno-enforce-eh-specs"
			"-ffor-scope"
			"-fno-for-scope"
			"-fno-gnu-keywords"
			"-fno-implicit-templates"
			"-fno-implicit-inline-templates"
			"-fno-implement-inlines"
			"-fms-extensions"
			"-fnew-inheriting-ctors"
			"-fnew-ttp-matching"
			"-fno-nonansi-builtins"
			"-fnothrow-opt"
			"-fno-operator-names"
			"-fno-optional-diags"
			"-fpermissive"
			"-fno-pretty-templates"
			"-frepo"
			"-fno-rtti"
			"-fsized-deallocation"
			"-ftemplate-backtrace-limit=n:int"
			"-ftemplate-depth=n"
			"-fno-threadsafe-statics"
			"-fuse-cxa-atexit"
			"-fno-weak"
			"-nostdinc++"
			"-fvisibility-inlines-hidden"
			"-fvisibility-ms-compat"
			"-fext-numeric-literals"
			"-Wabi=n:int"
			"-Wabi-tag"
			"-Wconversion-null"
			"-Wctor-dtor-privacy"
			"-Wdelete-non-virtual-dtor"
			"-Wliteral-suffix"
			"-Wmultiple-inheritance"
			"-Wnamespaces"
			"-Wnarrowing"
			"-Wnoexcept"
			"-Wnoexcept-type"
			"-Wnon-virtual-dtor"
			"-Wreorder"
			"-Wregister"
			"-Weffc++"
			"-Wstrict-null-sentinel"
			"-Wtemplates"
			"-Wno-non-template-friend"
			"-Wold-style-cast"
			"-Woverloaded-virtual"
			"-Wno-pmf-conversions"
			"-Wsign-promo"
			"-Wvirtual-inheritance"
		}
		
	}
}

